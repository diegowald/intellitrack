  <data name="sqlDBDataSource1.SQL" xml:space="preserve">
    <value>SELECT top 10 * FROM ( 
         SELECT     MTC_ID AS ID,  
         MTC_TRANSP_CAMION as IDTransponder,  
         MTC_NUM_CAMION as IDElemento,  
         MTC_CATEGORIA as Categoria,  
         MTC_NUM_EQUIPO as Equipo,  
         MTC_DIA_HORA as DIA_HORA,  
         MTC_LATITUD as Latitud,  
         MTC_LONGITUD as Longitud,  
         MTC_VELOCIDAD as Velocidad,  
         MTC_CURSO as Curso, 
         MTC_NOM_PUNTO as NOM_PUNTO, 
         MTC_NOM_CAMINO as NOM_CAMINO, 
         MTC_NOM_AREA as NOM_AREA,  
         MTC_NOM_FRENTE as NOM_FRENTE,  
         MTC_TAG_EQUIPO AS TAG_EQUIPO,  
         MTC_TRANSP_VOLT as TRANSP_VOLT, 
         MTC_TRANSP_TEMP as TRANSP_TEMP,  
         MTC_SENTIDO as SENTIDO 
         FROM         MOVIMIENTOS_TRANSPORTE_CANA 
         union ALL 
         SELECT     MFC_ID AS ID,  
         MFC_TRANSP_Frente as IDTransponder,  
         MFC_NOM_Frente as IDElemento,  
         MFC_CATEGORIA as Categoria,  
         NULL as Equipo,  
         MFC_DIA_HORA as DIA_HORA,  
         MFC_LATITUD as Latitud,  
         MFC_LONGITUD as Longitud,  
         MFC_VELOCIDAD as Velocidad,  
         MFC_CURSO as Curso, 
         NULL as NOM_PUNTO, 
         NULL as NOM_CAMINO,  
         MFC_NOM_AREA as NOM_AREA,  
         MFC_NOM_FRENTE as NOM_FRENTE,  
         NULL AS TAG_EQUIPO,  
         MFC_TRANSP_VOLT as TRANSP_VOLT, 
         MFC_TRANSP_TEMP as TRANSP_TEMP,  
         MFC_SENTIDO as SENTIDO 
         FROM         MOVIMIENTOS_FRENTE_COSECHA 
         union ALL 
         SELECT      
         MRE_ID AS ID,  
         MRE_TRANSP_CAMION as IDTransponder,  
         MRE_NUM_CAMION as IDElemento,  
         MRE_CATEGORIA as Categoria,  
         NULL as Equipo,  
         MRE_DIA_HORA as DIA_HORA,  
         MRE_LATITUD as Latitud,  
         MRE_LONGITUD as Longitud,  
         MRE_VELOCIDAD as Velocidad,  
         MRE_CURSO as Curso, 
         MRE_NOM_PUNTO as NOM_PUNTO, 
         MRE_NOM_CAMINO as NOM_CAMINO,  
         MRE_NOM_AREA as NOM_AREA,  
         NULL as NOM_FRENTE,  
         NULL AS TAG_EQUIPO,  
         MRE_TRANSP_VOLT as TRANSP_VOLT, 
         MRE_TRANSP_TEMP as TRANSP_TEMP,  
         MRE_SENTIDO as SENTIDO 
         FROM         MOVIMIENTOS_REGADORES 
       ) T</value>
